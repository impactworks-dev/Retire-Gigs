[[workflows.workflow]]
name = "Job Search & Notify"
author = 39403415
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"Starting job search and notification process...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = 'curl -X POST http://localhost:5000/api/send-job-notifications \'

[[workflows.workflow.tasks]]
task = "shell.exec"
args = '  -H "Content-Type: application/json" \'

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "  -H \"Authorization: Bearer YOUR_AUTH_TOKEN\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"Job notification process completed!\""

[[workflows.workflow]]
name = "Job Search & Notify"
author = 39403415
